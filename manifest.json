{
  "name": "Stack Overflow Extension",
  "version": "1.0",
  "description": "An extension for developers to search Stack Overflow with fewer clicks.",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Stack Overflow Extension",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/logo.svg",
      "32": "./images/logo.svg",
      "64": "./images/logo.svg",
      "128": "./images/logo.svg"
    }
  },
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "options.html"
  },
  "icons": {
    "16": "./images/logo.svg",
    "32": "./images/logo.svg",
    "64": "./images/logo.svg",
    "128": "./images/logo.svg"
  },
  "options_page": "options.html"
}
